SELECT I2.NAME, I.SUMMARY, I.DESCRIPTION
FROM ISSUES I JOIN ISSUELISTS I2 on I.ISSUELIST_ID = I2.ID;

SELECT I.SUMMARY, I.DESCRIPTION, U.FIRSTNAME, U.LASTNAME
FROM ISSUES I JOIN USERS U on U.ID = I.USER_ID_ASSIGNEDTO
ORDER BY I.SUMMARY;

SELECT U.FIRSTNAME, U.LASTNAME, COUNT(*) AS ISSUESS_COUNT
FROM USERS U JOIN ISSUES I on U.ID = I.USER_ID_ASSIGNEDTO
GROUP BY USER_ID_ASSIGNEDTO;